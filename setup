#!/usr/bin/bash

if [ "$1" = "env" ]; then
  bash 18c_env.sh
elif [ "$1" = "sw" ]; then
  bash 18c_linux_x64.sh
elif [ "$1" = "db" ]; then
  bash 18c_netca.sh
  bash 18c_dbca.sh
elif [ "$1" = "apex" ]; then
  bash 19c_apex.sh
elif [ "$1" = "all" ]; then
  bash 18c_linux_x64.sh
  bash 18c_netca.sh
  bash 18c_dbca.sh
  bash 19c_apex.sh
else
  echo "Usage: setup [env|sw|db|apex|all]"
  exit 2
fi

